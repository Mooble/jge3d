<keymap>
	<key ID="Key_LEFT" EVENT="KEY_PRESSED">rotateCamLeft</key>
	<key ID="Key_RIGHT" EVENT="KEY_PRESSED">rotateCamRight</key>
	<key ID="Key_UP" EVENT="KEY_PRESSED">rotateCamUp</key> 	
	<key ID="Key_DOWN" EVENT="KEY_PRESSED">rotateCamDown</key>
	<key ID="Key_PRIOR" EVENT="KEY_PRESSED">zoomCameraIn</key>
	<key ID="Key_NEXT" EVENT="KEY_PRESSED">zoomCameraOut</key>
	<key ID="Key_W" EVENT="KEY_PRESSED">playerForward</key>
	<key ID="Key_S" EVENT="KEY_PRESSED">playerBack</key>
	<key ID="Key_A" EVENT="KEY_PRESSED">playerLeft</key>
	<key ID="Key_D" EVENT="KEY_PRESSED">playerRight</key>
	<key ID="Key_SPACE" EVENT="KEY_PRESSED">playerJump</key>
	
	<key ID="Key_W" EVENT="KEY_RELEASED">playerStopForward</key>
	<key ID="Key_S" EVENT="KEY_RELEASED">playerStopBack</key>
	<key ID="Key_A" EVENT="KEY_RELEASED">playerStopLeft</key>
	<key ID="Key_D" EVENT="KEY_RELEASED">playerStopRight</key>
</keymap>