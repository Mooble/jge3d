<keymap>
	<keyboard>
		<key ID="LEFT" EVENT="PRESSED">rotateCamLeft</key>
		<key ID="RIGHT" EVENT="PRESSED">rotateCamRight</key>
		<key ID="UP" EVENT="PRESSED">rotateCamUp</key>
		<key ID="DOWN" EVENT="PRESSED">rotateCamDown</key>
		<key ID="PRIOR" EVENT="PRESSED">zoomCameraIn</key>
		<key ID="NEXT" EVENT="PRESSED">zoomCameraOut</key>
		<key ID="W" EVENT="PRESSED">playerForward</key>
		<key ID="S" EVENT="PRESSED">playerBack</key>
		<key ID="A" EVENT="PRESSED">playerLeft</key>
		<key ID="D" EVENT="PRESSED">playerRight</key>
		<key ID="SPACE" EVENT="PRESSED">playerJump</key>
		<key ID="W" EVENT="RELEASED">playerStopForward</key>
		<key ID="S" EVENT="RELEASED">playerStopBack</key>
		<key ID="A" EVENT="RELEASED">playerStopLeft</key>
		<key ID="D" EVENT="RELEASED">playerStopRight</key>
	</keyboard>
	<mouse>
		<event EVENT="MOVED"></event>
		<event EVENT="CLICKED"></event>
	</mouse>
</keymap>
